{
  "name": "ai-primitives",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "turbo run build:packages",
    "build": "turbo run build && pnpm build:next",
    "build:packages": "turbo run build:packages",
    "build:next": "pnpm build:content && tsx scripts/generate-api-schema.ts && cross-env NODE_OPTIONS='--max-old-space-size=6144 --no-deprecation' next build",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "build:filtered": "turbo run build --filter=$npm_config_filter && pnpm build:next:filtered",
    "build:next:filtered": "pnpm build:content && tsx scripts/generate-api-schema.ts && cross-env NODE_OPTIONS='--max-old-space-size=6144 --no-deprecation' next build",
    "build:triggers": "pnpm --filter=triggers.do build && pnpm build:next",
    "dev": "pnpm dev:content & cross-env NODE_OPTIONS='--max-old-space-size=6144 --no-deprecation' next dev --turbo",
    "dev:email": "email dev",
    "dev:turbo": "turbo dev --no-cache --concurrency 16 --continue",
    "eval:dev": "evalite watch",
    "eval": "evalite run",
    "experiments": "evalite watch experiments",
    "download:payload-docs": "FIRECRAWL_API_KEY=$FIRECRAWL_API_KEY node scripts/download-payload-docs.js",
    "download:payload-docs-direct": "node scripts/download-payload-docs-direct.js",
    "lint": "turbo lint",
    "lint:file": "eslint",
    "semantic-release": "node scripts/release.js",
    "semantic-release-sdks": "node scripts/release-sdks.js",
    "semantic-release-pkgs": "node scripts/release-pkgs.js",
    "semantic-release-next": "node scripts/release-next.js",
    "semantic-release-pkgs-next": "node scripts/release-pkgs-next.js",
    "semantic-release-sdks-next": "node scripts/release-sdks-next.js",
    "version-pr": "node scripts/release.js --dry-run",
    "version-pr-sdks": "node scripts/release-sdks.js --dry-run",
    "version-pr-pkgs": "node scripts/release-pkgs.js --dry-run",
    "version-pr-next": "node scripts/release-next.js --dry-run",
    "version-pr-pkgs-next": "node scripts/release-pkgs-next.js --dry-run",
    "version-pr-sdks-next": "node scripts/release-sdks-next.js --dry-run",
    "release": "pnpm build && pnpm semantic-release",
    "release-sdks": "pnpm build && pnpm semantic-release-sdks",
    "release-pkgs": "pnpm build && pnpm semantic-release-pkgs",
    "release-next": "pnpm build && pnpm semantic-release-next",
    "release-pkgs-next": "pnpm build && pnpm semantic-release-pkgs-next",
    "release-sdks-next": "pnpm build && pnpm semantic-release-sdks-next",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types && pnpm sync:sdk-types",
    "generate:zapier-apps": "tsx scripts/generate-zapier-apps.ts",
    "generate:cli-docs": "tsx scripts/generate-cli-docs.ts",
    "sync:sdk-types": "tsx scripts/sync-sdk-types.ts",
    "sync:sdk-docs": "node scripts/sync-sdk-docs.js",
    "generate:backlog": "tsx scripts/generateBacklog.ts",
    "payload": "cross-env NODE_OPTIONS='--max-old-space-size=6144 --no-deprecation' payload",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=6144 --no-deprecation' next start",
    "build:content": "velite",
    "dev:content": "velite --watch",
    "prettier-check": "prettier --check \"**/*.{js,ts,tsx,md,mdx}\"",
    "prettier-fix": "prettier --write \"**/*.{js,ts,tsx,md,mdx}\"",
    "test": "turbo test",
    "test:watch": "vitest",
    "test:unit": "vitest run --exclude tests/e2e/ --exclude tests/chromatic/",
    "test:e2e": "playwright test tests/e2e-playwright",
    "test:visual": "playwright test tests/chromatic",
    "test:chromatic": "npx playwright test tests/chromatic && npx chromatic --playwright --project-token=${CHROMATIC_PROJECT_TOKEN}",
    "clean": "turbo clean",
    "lint:legacy": "cross-env NODE_OPTIONS='--max-old-space-size=6144 --no-deprecation' next lint",
    "test:legacy": "vitest run",
    "test:seed": "vitest run tests/seed.test.ts",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,mdx}\"",
    "seed": "tsx scripts/seed.ts",
    "download:chromatic": "node scripts/download-chromatic-docs.js",
    "prepare": "husky"
  },
