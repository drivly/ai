{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build && pnpm build:next",
    "build:next": "pnpm build:content && cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "pnpm dev:content & cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:turbo": "turbo dev --no-cache --concurrency 16 --continue",
    "lint": "turbo lint",
    "changeset": "changeset",
    "version": "changeset version && pnpm install --lockfile-only",
    "release": "pnpm build && changeset publish",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:backlog": "tsx scripts/generateBacklog.ts",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "build:content": "velite",
    "dev:content": "velite --watch",
    "prettier-check": "prettier --check \"**/*.{js,ts,tsx,md,mdx}\"",
    "prettier-fix": "prettier --write \"**/*.{js,ts,tsx,md,mdx}\"",
    "test": "turbo test",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "clean": "turbo clean",
    "lint:legacy": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "test:legacy": "vitest run",
    "test:seed": "vitest run tests/seed.test.ts",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,mdx}\"",
    "seed": "tsx scripts/seed.ts"
  },
  "dependencies": {
    "@ai-primitives/data": "workspace:*",
    "@graphdl/core": "^0.1.6",
    "@payloadcms/db-mongodb": "^3.28.1",
    "@payloadcms/next": "^3.28.1",
    "@payloadcms/payload-cloud": "^3.28.1",
    "@payloadcms/plugin-multi-tenant": "^3.28.1",
    "@payloadcms/richtext-lexical": "^3.28.1",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/postcss": "^4.0.17",
    "@vercel/analytics": "^1.5.0",
    "@vercel/functions": "^2.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^4.2.0",
    "ai-functions": "^0.3.0",
    "ai-providers": "workspace:*",
    "ai-workflows": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clickable-apis": "workspace:*",
    "clsx": "^2.1.1",
    "composio-core": "^0.5.31",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "dedent": "^1.5.3",
    "graphdl.js": "^0.1.0",
    "graphql": "^16.8.1",
    "lucide-react": "^0.485.0",
    "murmurhash": "^2.0.1",
    "next": "15.2.3",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "nextra": "^4.2.17",
    "nextra-theme-docs": "^4.2.17",
    "object-hash": "^3.0.0",
    "payload": "^3.28.1",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "sharp": "0.32.6",
    "sonner": "^2.0.2",
    "sqids": "^0.3.0",
    "svix": "^1.62.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.17",
    "ts-morph": "^25.0.1",
    "tsx": "^4.19.3",
    "tw-animate-css": "^1.2.5",
    "unified": "^11.0.5",
    "velite": "^0.2.2",
    "yaml": "^2.2.2",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/eslintrc": "^3.2.0",
    "@octokit/webhooks": "^13.7.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^22.5.4",
    "@types/object-hash": "^3.0.6",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.2",
    "ignore-loader": "^0.1.2",
    "jsdom": "^26.0.0",
    "node-mocks-http": "^1.16.2",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "null-loader": "^4.0.1",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "5.7.3",
    "vitest": "^3.0.9"
  },
  "engines": {
    "node": "^20 ||^22",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "esbuild",
      "sharp"
    ]
  },
  "workspaces": [
    "api/*",
    "collections",
    "docs",
    "data",
    "dash",
    "sdk/*",
    "web/*",
    "pkgs/*",
    "workers/*",
    "workflows"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 180,
    "tabWidth": 2,
    "semi": false,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": true
  },
  "packageManager": "pnpm@10.7.0"
}
