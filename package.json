{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build && pnpm build:next",
    "build:next": "pnpm build:content && cross-env NODE_OPTIONS=--no-deprecation next build",
    "build:filtered": "turbo run build --filter=$npm_config_filter && pnpm build:next:filtered",
    "build:next:filtered": "pnpm build:content && cross-env NODE_OPTIONS=--no-deprecation next build",
    "build:triggers": "pnpm --filter=triggers.do build && pnpm build:next",
    "dev": "pnpm dev:content & cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:turbo": "turbo dev --no-cache --concurrency 16 --continue",
    "lint": "turbo lint",
    "lint:file": "eslint",
    "semantic-release": "node scripts/release.js",
    "version-pr": "node scripts/release.js --dry-run",
    "release": "pnpm build && pnpm semantic-release",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types && pnpm sync:sdk-types && pnpm tsx scripts/generate-api-docs-standalone.ts",
    "sync:sdk-types": "tsx scripts/sync-sdk-types.ts",
    "sync:sdk-docs": "node scripts/sync-sdk-docs.js",
    "generate:backlog": "tsx scripts/generateBacklog.ts",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "build:content": "velite",
    "dev:content": "velite --watch",
    "prettier-check": "prettier --check \"**/*.{js,ts,tsx,md,mdx}\"",
    "prettier-fix": "prettier --write \"**/*.{js,ts,tsx,md,mdx}\"",
    "test": "turbo test",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config tests/vitest.e2e.config.ts",
    "test:chromatic": "chromatic --playwright --project-token=${CHROMATIC_PROJECT_TOKEN} --config=playwright.config.ts",
    "clean": "turbo clean",
    "lint:legacy": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "test:legacy": "vitest run",
    "test:seed": "vitest run tests/seed.test.ts",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,mdx}\"",
    "seed": "tsx scripts/seed.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-primitives/data": "workspace:*",
    "@ai-sdk/openai": "^1.0.10",
    "@clickhouse/client-web": "^1.11.0",
    "@code-hike/mdx": "^0.9.0",
    "@drivly/payload-agent": "workspace:*",
    "@drivly/ui": "workspace:*",
    "@graphdl/core": "^0.1.6",
    "@ip-location-db/asn": "^2.3.2025040218",
    "@payload-auth/better-auth-db-adapter": "workspace:*",
    "@payload-auth/better-auth-plugin": "workspace:*",
    "@payloadcms/db-mongodb": "^3.33.0",
    "@payloadcms/email-resend": "^3.33.0",
    "@payloadcms/next": "^3.33.0",
    "@payloadcms/payload-cloud": "^3.33.0",
    "@payloadcms/plugin-multi-tenant": "^3.33.0",
    "@payloadcms/plugin-stripe": "^3.33.0",
    "@payloadcms/richtext-lexical": "^3.33.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/functions": "^2.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "agentic.md": "^0.1.0",
    "ai": "^4.2.0",
    "ai-functions": "workspace:*",
    "ai-providers": "workspace:*",
    "ai-workflows": "^1.1.0",
    "better-auth": "1.2.4",
    "class-variance-authority": "^0.7.1",
    "clickable-apis": "workspace:*",
    "clsx": "^2.1.1",
    "codehike": "^1.0.5",
    "coffee-script": "^1.12.7",
    "composio-core": "^0.5.31",
    "cross-env": "^7.0.3",
    "csv-parse": "^5.6.0",
    "date-fns": "^3.6.0",
    "dedent": "^1.5.3",
    "geolib": "^3.3.4",
    "graphdl.js": "^0.1.0",
    "graphql": "^16.8.1",
    "isolated-vm": "^5.0.4",
    "lucide-react": "^0.483.0",
    "marked": "^15.0.7",
    "murmurhash": "^2.0.1",
    "next": "15.2.4",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "nextra": "^4.2.17",
    "nextra-theme-docs": "^4.2.17",
    "object-hash": "^3.0.0",
    "payload": "^3.33.0",
    "payload-kanban-board": "^3.0.24",
    "postal-mime": "^2.4.3",
    "posthog-js": "^1.234.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "sharp": "0.32.6",
    "sqids": "^0.3.0",
    "svix": "^1.62.0",
    "tailwind-merge": "^2.6.0",
    "tailwind-scrollbar-hide": "^2.0.0",
    "ts-morph": "^25.0.1",
    "tsx": "^4.19.3",
    "tw-animate-css": "^1.2.4",
    "ua-parser-js": "^2.0.3",
    "unified": "^11.0.5",
    "velite": "^0.2.2",
    "vm2": "^3.9.19",
    "yaml": "^2.7.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@chromatic-com/playwright": "^0.12.0",
    "@eslint/eslintrc": "^3.2.0",
    "@octokit/webhooks": "^13.7.5",
    "@tailwindcss/postcss": "^4.0.15",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/geolib": "^2.0.25",
    "@types/node": "^22.5.4",
    "@types/object-hash": "^3.0.6",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/ua-parser-js": "^0.7.39",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.2",
    "husky": "^9.1.7",
    "ignore-loader": "^0.1.2",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "multi-semantic-release": "^3.0.2",
    "node-mocks-http": "^1.16.2",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "null-loader": "^4.0.1",
    "playwright": "^1.51.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4.0.15",
    "turbo": "^2.4.4",
    "typescript": "5.7.3",
    "vitest": "^3.0.9"
  },
  "engines": {
    "node": "^20 ||^22",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "esbuild",
      "sharp"
    ]
  },
  "workspaces": [
    "api/*",
    "collections",
    "docs",
    "data",
    "dash",
    "sdk/*",
    "sdks/*",
    "web/*",
    "pkgs/*",
    "workers/*",
    "workflows"
  ],
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 180,
    "tabWidth": 2,
    "semi": false,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": true
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "pnpm lint:file",
      "prettier --write"
    ],
    "**/*.{md,mdx}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.7.0"
}
