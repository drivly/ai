{
  "name": "ai-providers",
  "version": "0.2.0",
  "description": "Provider router for AI models including OpenAI, Anthropic, and Google",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "dotenvx run -- npx tsx watch src/demo.ts",
    "clean": "rimraf dist",
    "build": "tsc",
    "lint": "tsc --noEmit",
    "prepublishOnly": "pnpm clean && pnpm build",
    "typecheck": "tsc --noEmit",
    "test:local": "dotenvx run -- vitest"
  },
  "type": "module",
  "packageManager": "pnpm@10.6.5",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.0",
    "@ai-sdk/google": "^1.2.1",
    "@ai-sdk/google-vertex": "^2.2.14",
    "@ai-sdk/openai": "^1.3.16",
    "ai": "^4.3.9",
    "composio-core": "^0.5.31",
    "language-models": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "eslint-config": "0.1.0",
    "rimraf": "^6.0.1",
    "tsconfig": "0.0.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
