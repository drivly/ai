{
  "name": "ai-database",
  "version": "0.1.0",
  "description": "Direct interface to Payload CMS with database.do compatibility",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "format": "prettier --write src",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "payload",
    "cms",
    "database",
    "api"
  ],
  "author": "Drivly",
  "license": "MIT",
  "homepage": "https://mdx.org.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/ai"
  },
  "bugs": {
    "url": "https://github.com/drivly/ai/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "apis.do": "workspace:*",
    "payload": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
