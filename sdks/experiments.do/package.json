{
  "name": "experiments.do",
  "version": "0.1.0",
  "description": "A framework for testing hypotheses, measuring outcomes, and iteratively improving your AI applications",
  "keywords": [
    "experiments",
    "a/b testing",
    "feature flags",
    "hypothesis testing",
    "ai",
    "optimization"
  ],
  "homepage": "https://experiments.do",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/ai.git",
    "directory": "sdks/experiments.do"
  },
  "author": "Drivly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drivly/ai/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "postbuild": "tsc --emitDeclarationOnly --declaration --outDir dist",
